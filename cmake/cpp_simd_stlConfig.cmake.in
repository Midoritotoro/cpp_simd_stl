@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

macro(import_targets type)
    if(NOT EXISTS "${CMAKE_CURRENT_LIST_DIR}/cpp_simd_stlTargets-${type}.cmake")
        set(${CMAKE_FIND_PACKAGE_NAME}_NOT_FOUND_MESSAGE
            "cpp_simd_stl ${type} libs requested, but no found")
        set(${CMAKE_FIND_PACKAGE_NAME}_FOUND OFF)
        return()
    endif()

    include("${CMAKE_CURRENT_LIST_DIR}/cpp_simd_stlTargets-${type}.cmake")
endmacro()

if(NOT TARGET cpp_simd_stl::cpp_simd_stl)
    if (CMAKE_BUILD_TYPE STREQUAL "Debug")
        set(type "debug")
    elseif (CMAKE_BUILD_TYPE STREQUAL "Release")
        set(type "release")
    endif()

    import_targets(${type})
    check_required_components(cpp_simd_stl)
    message("-- Found ${type} cpp_simd_stl (version ${${CMAKE_FIND_PACKAGE_NAME}_VERSION})")
endif()